        "command" : [
          "import numpy as np",
          "df= pd.read_csv(**kwargs)",
          "df['idx_sentence'] = (df['pos']=='_').cumsum(); df=df[df['pos']!='_']",
          "df['idx_word'] = np.arange(df.shape[0])",
          "def check_join(x) : a= str(x['subword_join']).split('+'); b=str(x['pos_etc']).split('+'); return list(zip(a,b)) if len(a)==len(b) else [(a,b)]",
          "df['subword_pos_join'] = df[['subword_join','pos_etc']].apply(check_join , axis=1)",
          "df = df.explode('subword_pos_join').reset_index()",
          "df = df.assign(idx_subword=np.arange(df.shape[0]))",
          "df['subword_pos_join'] = df['subword_pos_join'].apply(list)",
          "df[['subword','pos_etc']] = pd.DataFrame(df['subword_pos_join'].tolist(), columns = ['subword','pos_etc'], index=df.index)",
          "df = df[['word', 'subword', 'pos', 'idx_sentence', 'idx_word', 'idx_subword']]",
          "df_tmp = df.set_index(['idx_word',df.index])['idx_subword']",
          "idx_subword = df_tmp.sub(df_tmp.min(level='idx_word'),level=0).reset_index()['idx_subword']",
          "df = df.assign(idx_subword = idx_subword)",
          "df_tmp = df.set_index(['idx_sentence',df.index])['idx_word']",
          "idx_word = df_tmp.sub(df_tmp.min(level='idx_sentence'),level=0).reset_index()['idx_word']",
          "df = df.assign(idx_word = idx_word)"
